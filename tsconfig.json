/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.main.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@core/*":["src/app/core/*"],
      "@product/*":["src/app/product/*"],
      "@cart/*":["src/app/cart/*"],
      "@shared/*":["src/app/shared/*"]
    } 
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
